{
  "dashboard": {
    "id": null,
    "title": "MMF Distributed Tracing Analysis",
    "tags": ["mmf", "tracing", "opentelemetry", "correlation"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "Trace Duration Distribution",
        "type": "histogram",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(traces_duration_ms_bucket[5m])) by (le, service_name))",
            "legendFormat": "P50 {{service_name}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(traces_duration_ms_bucket[5m])) by (le, service_name))",
            "legendFormat": "P95 {{service_name}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(traces_duration_ms_bucket[5m])) by (le, service_name))",
            "legendFormat": "P99 {{service_name}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "viz": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "ms"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Service Dependencies",
        "type": "nodeGraph",
        "targets": [
          {
            "expr": "sum by (client_service, server_service) (rate(traces_client_server_duration_sum[5m]))",
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 3,
        "title": "Trace Error Rate by Service",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(traces_total{status=\"error\"}[5m]) / rate(traces_total[5m]) * 100",
            "legendFormat": "{{service_name}} Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "viz": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 18
        }
      },
      {
        "id": 4,
        "title": "Correlation ID Coverage",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(traces_total{correlation_id!=\"\"}[5m])) / sum(rate(traces_total[5m])) * 100",
            "legendFormat": "Correlation Coverage",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 18
        }
      },
      {
        "id": 5,
        "title": "Span Count by Operation",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(traces_spans_total[5m]) by (operation_name, service_name)",
            "legendFormat": "{{service_name}} - {{operation_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 6,
        "title": "Recent Traces with Correlation IDs",
        "type": "table",
        "targets": [
          {
            "expr": "topk(50, max_over_time(traces_duration_ms{correlation_id!=\"\"}[1h]))",
            "legendFormat": "",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "list",
              "filterable": true
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "correlation_id"
              },
              "properties": [
                {
                  "id": "custom.width",
                  "value": 300
                },
                {
                  "id": "links",
                  "value": [
                    {
                      "title": "View in Jaeger",
                      "url": "http://jaeger:16686/search?service=${__data.fields.service_name}&tags={\"mmf.correlation.correlation_id\":\"${__data.fields.correlation_id}\"}"
                    }
                  ]
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 34
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "multi": false,
          "name": "service_name",
          "options": [],
          "query": "label_values(traces_total, service_name)",
          "refresh": 1,
          "regex": "",
          "sort": 0,
          "type": "query"
        },
        {
          "current": {
            "selected": false,
            "text": "",
            "value": ""
          },
          "hide": 0,
          "name": "correlation_id",
          "options": [],
          "query": "",
          "type": "textbox"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "icon": "external link",
        "includeVars": true,
        "keepTime": true,
        "tags": [],
        "targetBlank": true,
        "title": "Jaeger UI",
        "tooltip": "Open Jaeger UI for detailed trace analysis",
        "type": "link",
        "url": "http://jaeger:16686"
      }
    ]
  }
}
