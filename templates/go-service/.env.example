# Environment variables
ENVIRONMENT=development
PORT=8080
LOG_LEVEL=info

# Database configuration (if included)
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=password
DATABASE_NAME={{ service_name }}
DATABASE_SSL_MODE=disable

# Redis configuration (if included)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT configuration (if included)
JWT_SECRET=your-secret-key
JWT_EXPIRES_IN=24h

# Security
CORS_ORIGINS=*
RATE_LIMIT=100

# Monitoring
METRICS_PATH=/metrics
HEALTH_PATH=/health
